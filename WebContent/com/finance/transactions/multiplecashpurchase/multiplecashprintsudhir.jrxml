		<queryString>
			<![CDATA[select a.*,round($P{opnbal}-coalesce(@i:=@i+a.total,0),2) netamount from(SELECT ma.refname vendor,d.invno,date_format(d.invdate,'%d-%m-%Y')invdate,t.account accounts,
