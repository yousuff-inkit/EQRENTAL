<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="limoinvoice" extends="struts-default">
			<action name="saveLimoInvoice" class="com.limousine.limoinvoice.ClsLimoInvoiceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >limoInvoice.jsp</result>
				<result name="fail">limoInvoice.jsp</result>
			</action>
			<action name="printLimoInvoice" class="com.limousine.limoinvoice.ClsLimoInvoiceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result> 
			</action>
		</package>	
</struts>
