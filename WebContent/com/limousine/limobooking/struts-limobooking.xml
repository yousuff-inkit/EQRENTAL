<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="limobooking" extends="struts-default">
			<action name="saveLimoBooking" class="com.limousine.limobooking.ClsLimoBookingAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >limoBooking.jsp</result>
				<result name="fail">limoBooking.jsp</result>
			</action>
		</package>	
</struts>
