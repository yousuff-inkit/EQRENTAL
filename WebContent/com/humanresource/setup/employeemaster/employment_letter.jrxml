			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement x="11" y="129" width="29" height="20" uuid="c4c5eb6e-c356-4eb9-acf2-6ef8c35e53ab"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="5"/>
				</textElement>
				<textFieldExpression><![CDATA["TO "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement x="11" y="156" width="279" height="30" uuid="15237ea4-dd19-4028-abab-f0884b5ca563"/>
				<textElement verticalAlignment="Top">
					<font size="8"/>
					<paragraph leftIndent="5"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{name}+"\n"+$F{pmaddr}+"\n"+"Phone:"+$F{pmmob}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="210" width="249" height="20" uuid="ca16fd98-0934-48d2-b188-27ae6d10d17d"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="5"/>
				</textElement>
				<textFieldExpression><![CDATA["Dear Mr. "+$F{name}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="240" width="542" height="25" uuid="45646d66-fb63-4fbf-b176-96bbf4b22d60"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Further to our interview and to the discussions we had with you regarding your employment with us we are pleased to offer you the position of " +$F{designation}+ " with us. We trust that your knowledge, skills and experiences shall contribute to our most valuable assets."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="264" width="542" height="26" uuid="05a17f43-8f10-40e9-92b2-ba78516c73bd"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["As discussed during your interviews, you will report directly to the MD and others that may deem to be necessary to achieve the organizational objectives."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="288" width="542" height="28" uuid="642188ad-a6fc-4b9f-a524-78adf827d206"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["However you will also be required to focus on other areas of our operation, which may be ne-cessary for the control of our operations. A detailed job description of your position shall be available at the time of joining. "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="318" width="140" height="15" uuid="e1fbf6c2-6b07-42d2-af72-c8af9944f1f1"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["1. DATE OF JOINING"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="334" width="541" height="40" uuid="e2f16fd6-41cf-497d-8718-9926fa049684"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Your date of work commencement shall be subject to the approval of your employment visa by the UAE labor ministry and passing the pre-employment medical examination as stipulated by the law. As part of the Company Policy, you are required to sign the Employee Confidential Agreement letter, which must be returned to us upon signing. "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="9" y="392" width="541" height="29" uuid="525bab5c-4351-460e-a12d-47111ea05e87"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Your monthly salary shall be  "+$F{salary}+"  AED all inclusive for the initial one year, there after a revision of your total package is available based on your performance. "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="375" width="140" height="15" uuid="623e7e7a-cd3c-4062-80a9-02f38acdb78d"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["2. COMPENSATION"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="423" width="140" height="15" uuid="452ad75c-f8fc-49ab-a50d-d4339af30e6d"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["3. OFFICE HOURS"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="440" width="541" height="30" uuid="69578f76-f499-4821-97c1-8d3d2d0f61af"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Office hours are weekdays between 8.00am and 5.30pm (Saturday to Thursday) with an unpaid lunch hour to be taken at a mutually agreed time. It should be understood that you might need to work outside these hours on occasions in order to need the objective of the organization."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="472" width="140" height="15" uuid="4848bd61-f94d-4472-8534-8a16bef4655b"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["4. PROBATION PERIOD "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="489" width="541" height="12" uuid="2bc071ac-a19b-431d-aba6-aadea9d7addf"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["(a) A performance review will be undertaken during the initial six-month period prior to con-firmation of your ongoing full time employment."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="503" width="541" height="21" uuid="e860047c-7ced-465b-96e2-82b3f2fa452c"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["(b) This offer will be based on your work performance against the agreed criteria as per your job description. During this probationary period, “Our Team” will assist you, as necessary, to prove yourself suitable for the position."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="526" width="541" height="15" uuid="3cd3e4a2-b909-465b-8263-01c0a97cc143"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["(c) If you are unsuitable you will not be offered a full time position and your services will be terminated with a written notice."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="543" width="140" height="15" uuid="4b4bdad1-eb57-415e-876c-77c1a3f6b89b"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["5. TERMINATION"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="560" width="541" height="16" uuid="cb92ad12-55e1-4e9d-b449-2f7b6948f133"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["During the probationary period, termination of employment can be effected by either party giv-ing one month notice in writing. "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="579" width="541" height="26" uuid="6abf88c6-cbf3-48df-9376-fbf54e1bb2b6"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Upon confirmation of your employment after the successful completion of probationary period, either party may terminate the contract of service by giving a 1-month notice or payment in lieu equivalent to 1 month’s gross salary."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="607" width="541" height="25" uuid="2f0e5aae-9128-47dd-82c4-625027805562"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["During the course of your employment, should your performance fail to meet the expectation of the company, you shall be provided with a written warning letter which will be monitored for a period in order to enhance the performance."]]></textFieldExpression>
			</textField>
		</band>
		<band height="648">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="26" width="541" height="30" uuid="bd808d11-1ded-4be5-b208-e579f9706859"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["Should you fail to improve, your services shall be terminated with immediate effect by charging all costs of your employment with us towards your account."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="114" width="189" height="15" uuid="636e7116-e7b3-41fc-ada7-2b52c61d46c8"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["6. LEAVES & PUBLIC HOLIDAYS "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="60" width="99" height="15" uuid="96527405-7250-44e5-9095-3f00287a795d"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["ANNUAL LEAVE: "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="77" width="541" height="34" uuid="e4a74984-0dc0-4ec8-be1c-c177c2cdc989"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["You are entitled to 30days paid annual leave, with a one way ticket to your home country at the completion of 12 months employment. Annual Leave will be taken at a mutually convenient time each year. It is company policy to ensure that annual leave does not accumulate beyond one year’s entitlement."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="131" width="99" height="15" uuid="e00097c7-f1a2-4bdd-b4c3-43c53308d436"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["SPECIAL LEAVE:"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="148" width="541" height="28" uuid="091774ae-1b57-417a-bd10-6c6ec594c597"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["In special circumstances you may be granted leave on application to your manager. This leave is at the discretion of your manager and may be paid or unpaid depending on the circumstances.All leave types should be applied for on the appropriate leave applications and approved by your manager."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="178" width="119" height="15" uuid="439c5854-a5d4-4a31-a564-15d5552d8e90"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["PUBLIC HOLIDAYS:"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="195" width="541" height="18" uuid="774fd502-0499-426e-85e1-155c6c11baa9"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["(a) You will be able to take all gazette public holidays without loss of pay."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="10" y="215" width="541" height="18" uuid="fa3792c7-7243-4d26-be5d-8ad930b8c3cf"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
				<textFieldExpression><![CDATA["(b) If required to work on a public holiday, you will be granted the equivalent time off or payment as agreed."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement positionType="Float" x="11" y="235" width="119" height="15" uuid="6275244c-8fd3-4a40-8449-554a457577b8"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
					<paragraph leftIndent="3"/>
				</textElement>
