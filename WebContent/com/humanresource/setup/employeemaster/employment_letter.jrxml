	<parameter name="imgfooter" class="java.lang.String"/>
	<queryString>
		<![CDATA[select m.codeNo empid,coalesce(chk1.docIdnum,'')licenseno,coalesce(chk2.docidnum,'')passportno,round(sum(el.addition),2)salary,m.doc_no,dsg.desc1 designation,date_format(m.date,'%d-%m-%Y')date,m.name,date_format(m.dtjoin,'%d-%m-%Y')dtjoin, m.pmaddr, m.pmmob, m.city, m.state,n.nation,
m.pincode from hr_empm m left join my_head h on m.acno=h.doc_no left join my_natm n on m.nationId=n.doc_no left join hr_setdesig dsg on m.desc_id=dsg.doc_no left join hr_setdept dep on m.dept_id=dep.doc_no
left join hr_setpaycat cat on m.pay_catid=cat.doc_no left join my_brch b on m.brhid=b.doc_no left join my_locm l on l.brhid=b.doc_no left join hr_setagent ag on m.agent_id=ag.doc_no
left join my_comp c on b.cmpid=c.doc_no left join (select rdocno,docidnum from hr_empdoc where docid=5 group by rdocno) chk1 on chk1.rdocno=m.doc_no
left join (select rdocno,docidnum from hr_empdoc where docid=1 group by rdocno) chk2 on chk2.rdocno=m.doc_no
left join hr_empsal el on el.rdocno=m.doc_no where m.doc_no=$P{docno};]]>
	</queryString>
