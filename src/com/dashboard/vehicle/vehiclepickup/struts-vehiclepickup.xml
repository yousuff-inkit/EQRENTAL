<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="vehiclepickup" extends="struts-default">
			
			<action name="saveVehiclePickup" class="com.dashboard.vehicle.vehiclepickup.ClsVehiclePickUpAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >vehiclePickUp.jsp</result>
				<result name="fail">vehiclePickUp.jsp</result>
			</action>			
			<action name="vehiclePickUpPrint" class="com.dashboard.vehicle.vehiclepickup.ClsVehiclePickUpAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">vehPickupPrint.jsp</result>
			</action>
		</package>	
</struts>