<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="driveragmtcreate" extends="struts-default">
			<action name="saveDriverAgmtCreate" class="com.dashboard.operations.driveragmtcreate.ClsDriverAgmtCreateAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >driverAgmtCreate.jsp</result>
				<result name="fail">driverAgmtCreate.jsp</result>
				
			</action>
			<action name="printdriveragmt" class="com.dashboard.operations.driveragmtcreate.ClsDriverAgmtCreateAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">driverAgmtPrint.jsp</result>
			</action>

		</package>	
</struts>

