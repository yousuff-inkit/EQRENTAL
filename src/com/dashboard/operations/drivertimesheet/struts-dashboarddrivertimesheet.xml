<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="drivertimesheet" extends="struts-default">
			<action name="saveDashboardDriverTimeSheet" class="com.dashboard.operations.drivertimesheet.ClsDriverTimeSheetAction" method="saveAction">
