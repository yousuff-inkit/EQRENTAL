<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="vehicleinsurposting" extends="struts-default">
			<action name="saveVehicleInsurancePosting" class="com.dashboard.operations.vehicleinsurposting.ClsVehicleInsurPostingAction" method="saveAction">
