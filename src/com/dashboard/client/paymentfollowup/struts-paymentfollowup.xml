<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="paymentfollowup" extends="struts-default">
			
			<action name="printOutstandingsStatement" class="com.dashboard.client.paymentfollowup.ClsPaymentFollowUpAction" method="printAction">
				<interceptor-ref name="params"/>
			</action>
			
			<action name="printRAWiseBalance" class="com.dashboard.client.paymentfollowup.ClsPaymentFollowUpAction" method="printRAAction">
				<interceptor-ref name="params"/>
			</action>
			<action name="sendPaymentSms" class="com.dashboard.client.paymentfollowup.ClsPaymentFollowUpAction" method="sendSmsAction">
				<interceptor-ref name="params"/>
			</action>
			
		</package>	
</struts>