<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="jobclose" extends="struts-default">
			<action name="saveLimoJobClose" class="com.dashboard.limousine.jobclose.ClsLimoJobCloseAction" method="saveAction">
