<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="dailyweekly" extends="struts-default">
			<action name="saveDailyWeeklyInvoice" class="com.dashboard.invoices.dailyweekly.ClsDailyWeeklyAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >invoice.jsp</result>
				<result name="fail">invoice.jsp</result>
				
			</action>

		</package>	
</struts>
