<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="payment" extends="struts-default">
			<action name="saveDashboardSalaryPayment" class="com.dashboard.humanresource.salarypayment.ClsSalaryPaymentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">salaryPayment.jsp</result>
				<result name="fail">salaryPayment.jsp</result>
				
			</action>

		</package>	
</struts>
