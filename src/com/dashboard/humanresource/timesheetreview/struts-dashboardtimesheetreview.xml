<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="dashboardtimesheetreview" extends="struts-default">
			<action name="saveDashboardTimeSheetReview" class="com.dashboard.humanresource.timesheetreview.ClsTimeSheetReviewAction" method="saveAction">
