<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="refundrequestmanagement" extends="struts-default">
			<action name="saveRefundManagement" class="com.dashboard.travel.refundrequestmanagement.ClsRefundRequestManagementAction" method="saveAction">
				<interceptor-ref name="params"/>      
				<result name="success">refundrequestmanagement.jsp</result>
				<result name="fail">refundrequestmanagement.jsp</result>     
			</action>   
	</package>	   
</struts> 