<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="hotelbookingportal" extends="struts-default">
			<action name="saveHotelBooking" class="com.dashboard.travel.hotelbookingportal.ClsHotelBookingPortalAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >hotelbookingportal.jsp</result>
				<result name="fail">hotelbookingportal.jsp</result>     
			</action>
	</package>	
</struts>