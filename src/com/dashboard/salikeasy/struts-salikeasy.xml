<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="salikeasy" extends="struts-default">
			
			<action name="printSalikToBeInvoiced" class="com.dashboard.salik.ClsSalikAction" method="printTobeInvoicedAction">
				<interceptor-ref name="params"/>
				<result name="print">salikToBeInvoicedPrint.jsp</result>
			</action>

			<action name="printSalikInvoice" class="com.dashboard.salik.ClsSalikAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">salikInvoicePrint.jsp</result>
			</action>			
			
				<action name="saveNotInvoicedSalikEasy" class="com.dashboard.salikeasy.ClsSalikEasyAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >notInvoicedEasy.jsp</result>
				<result name="fail">notInvoicedEasy.jsp</result>
				
			</action>	
		</package>	
</struts>