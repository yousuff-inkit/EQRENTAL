<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="balancesheet" extends="struts-default">
			
			<action name="printTForm" class="com.dashboard.accounts.balancesheet.ClsBalanceSheetAction" method="printTFormAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
			<action name="balancesheetlist" class="com.dashboard.accounts.balancesheet.ClsBalanceSheetAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
			<action name="balancesheetlist2" class="com.dashboard.accounts.balancesheet.ClsBalanceSheetAction" method="print2Action">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>	
		</package>	
</struts>