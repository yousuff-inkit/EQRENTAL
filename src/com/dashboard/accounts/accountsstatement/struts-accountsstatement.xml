<struts>
	<constant name="struts.devMode" value="true" />

	<package name="accountsstatement" extends="struts-default">
			
			<action name="printAccountsStatement" class="com.dashboard.accounts.accountsstatement.ClsAccountsStatementAction" method="printAction">
				<interceptor-ref name="params"/>
			</action>
			
			<action name="printARProjectWise" class="com.dashboard.accounts.accountsstatement.ClsAccountsStatementAction" method="printProjectWiseAction">
				<interceptor-ref name="params"/>
			</action>
			<action name="printRAWise" class="com.dashboard.accounts.accountsstatement.ClsAccountsStatementAction" method="printRAwiseAction">
				<interceptor-ref name="params"/>
			</action>
		</package>	
</struts>