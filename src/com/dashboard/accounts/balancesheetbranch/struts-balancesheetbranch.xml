			<action name="balancesheetlistbranch" class="com.dashboard.accounts.balancesheetbranch.ClsBalanceSheetAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>  
			</action>
			
			<action name="balancesheetlist2branch" class="com.dashboard.accounts.balancesheetbranch.ClsBalanceSheetAction" method="print2Action">
