<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="MR" extends="struts-default">
			<action name="saveActionmr" class="com.sales.InventoryTransfer.materialrequest.ClsMaterialrequestAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">materialrequest.jsp</result>
				<result name="fail">materialrequest.jsp</result>
			</action>
			
			
 
	 <action name="PRINTmr" class="com.sales.InventoryTransfer.materialrequest.ClsMaterialrequestAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print" >${url}</result> 
			</action>
			
			
		</package>	
		
		
		
		
</struts>