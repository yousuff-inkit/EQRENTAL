<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="transferRecept" extends="struts-default">
			<action name="savetransferRecept" class="com.sales.InventoryTransfer.InventoryTransferRecept.ClsTransferReceptAction" method="savetransferReceptAction">
				<interceptor-ref name="params"/>
				<result name="success" >InventoryTransferRecept.jsp</result>
				<result name="fail">InventoryTransferRecept.jsp</result>
			</action>
			
				
			 	<action name="printtransissuerptAction" class="com.sales.InventoryTransfer.InventoryTransferRecept.ClsTransferReceptAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">InvtransferReceiptPrint.jsp</result>
			</action>
			
			
			
		</package>	
		
		
		
</struts>