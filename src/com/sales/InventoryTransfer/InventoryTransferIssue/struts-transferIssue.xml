<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="transferIssue" extends="struts-default">
			<action name="savetransferIssue" class="com.sales.InventoryTransfer.InventoryTransferIssue.ClsTransferIssueAction" method="savetransferIssueAction">
				<interceptor-ref name="params"/>
				<result name="success" >InventoryTransferIssue.jsp</result>
				<result name="fail">InventoryTransferIssue.jsp</result>
			</action>
			
			
			 	<action name="printtransissueAction" class="com.sales.InventoryTransfer.InventoryTransferIssue.ClsTransferIssueAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">InvtransferPrint.jsp</result>
			</action>
			
			
			
		</package>	
		
		
		
</struts>