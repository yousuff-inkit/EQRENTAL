<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Joborder" extends="struts-default">
			<action name="saveJOD" class="com.salesandmarketing.Sales.joborder.ClsJobOrderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >joborder.jsp</result>
				<result name="fail">joborder.jsp</result>
			</action>
			
	 
 
			<action name="printjob" class="com.salesandmarketing.Sales.joborder.ClsJobOrderAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >joborderprint.jsp</result>
				 
			</action>
			
		</package>		
			
		
		
		
</struts>