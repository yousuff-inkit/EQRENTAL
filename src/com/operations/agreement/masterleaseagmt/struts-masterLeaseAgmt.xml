<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="masterleaseagmt" extends="struts-default">
			<action name="saveMasterLeaseAgmt" class="com.operations.agreement.masterleaseagmt.ClsMasterLeaseAgmtAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">masterLeaseAgmt.jsp</result>
				<result name="fail">masterLeaseAgmt.jsp</result>
			</action>
			<action name="printMasterLeaseAgmt" class="com.operations.agreement.masterleaseagmt.ClsMasterLeaseAgmtAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
		</package>	
</struts>
