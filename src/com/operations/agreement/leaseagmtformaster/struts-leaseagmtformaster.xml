<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveLeaseAgreement" />

	<package name="leaseagmtformaster" extends="struts-default">
			<action name="saveLeaseAgreementForMaster" class="com.operations.agreement.leaseagmtformaster.ClsLeaseAgmtForMasterAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >leaseAgmt.jsp</result>
				<result name="fail">leaseAgmt.jsp</result>
			</action>
			
				<action name="Leasemainprint" class="com.operations.agreement.leaseagreement.ClsLeaseAgreementAction" method="printActions">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>
			
				<action name="LeasemasterPrint" class="com.operations.agreement.leaseagreement.ClsLeaseAgreementAction" method="printActions">
			    <interceptor-ref name="params"/>
			    <result name="print">${url2}</result>
			</action>
			<action name="printLAGClosingSummary" class="com.operations.agreement.leaseagreement.ClsLeaseAgreementAction" method="printClosingSummaryAction">
			    <interceptor-ref name="params"/>
			    <result name="print">closingSummaryPrint.jsp</result>
			</action>
			<action name="leaseAgrementDetails" class="com.operations.agreement.leaseagreement.ClsLeaseAgreementAction" method="saveAction">
			    <interceptor-ref name="params"/>
			    <result name="success" >../../../../com/operations/agreement/leaseagreement/leaseAgmt.jsp</result>
			    <result name="fail">../../../../com/operations/agreement/leaseagreement/leaseAgmt.jsp</result>
   			</action>
   			<action name="printKmDetailsLease" class="com.operations.agreement.leaseagreement.ClsLeaseAgreementAction" method="printKm">
			    <interceptor-ref name="params"/>
			    <result name="print">kmPrint.jsp</result>
   			</action>
		</package>	
</struts>
