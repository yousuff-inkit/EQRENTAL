<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="rentalclosecarfare" extends="struts-default">
			<action name="saveRentalCloseCarFare" class="com.operations.agreement.rentalclosecarfare.ClsRentalCloseCarFareAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">rentalClose.jsp</result>
				<result name="fail">rentalClose.jsp</result>
			</action>
			<action name="printRentalClose" class="com.operations.agreement.rentalagreement.ClsRentalAgreementAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			<action name="printRAClosingSummaryCarfare" class="com.operations.agreement.rentalclosecarfare.ClsRentalCloseCarFareAction" method="printClosingSummaryAction">
			    <interceptor-ref name="params"/>
			    <result name="print">closingSummaryPrint.jsp</result>
			</action>
		</package>	
</struts>
