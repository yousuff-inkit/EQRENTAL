<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="otherRequest" extends="struts-default">
			<action name="saveOtherRequest" class="com.operations.commtransactions.otherrequest.ClsOtherRequestAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >otherRequest.jsp</result>
				<result name="fail">otherRequest.jsp</result>
			</action>
			
			<action name="printExtraServiceRequest" class="com.operations.commtransactions.otherrequest.ClsOtherRequestAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">extraServiceRequestPrint.jsp</result>
			</action>
			
		</package>	
</struts>