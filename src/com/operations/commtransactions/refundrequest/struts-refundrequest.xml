<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="refundrequest" extends="struts-default">       
			<action name="saveRefundRequest" class="com.operations.commtransactions.refundrequest.ClsRefundRequestAction" method="saveAction">
				<interceptor-ref name="params"/>         
				<result name="success">refundrequest.jsp</result>
				<result name="fail">refundrequest.jsp</result>
			</action>             
		</package>	         
</struts>