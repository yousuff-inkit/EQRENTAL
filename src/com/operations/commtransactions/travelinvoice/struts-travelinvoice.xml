<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="Travel" extends="struts-default">
			<action name="saveTravelInvoice" class="com.operations.commtransactions.travelinvoice.ClsTravelInvoiceAction" method="saveAction">
				<interceptor-ref name="params"/>         
				<result name="success">travelinvoice.jsp</result>
				<result name="fail">travelinvoice.jsp</result>
			</action>
			<action name="printtravelinvoice" class="com.operations.commtransactions.travelinvoice.ClsTravelInvoiceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>       
			</action>
		</package>	 
</struts>