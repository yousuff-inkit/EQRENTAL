<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="rentalRefund" extends="struts-default">
			<action name="saveRentalRefund" class="com.operations.commtransactions.rentalrefund.ClsRentalRefundAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >rentalRefund.jsp</result>
				<result name="fail">rentalRefund.jsp</result>
			</action>
			
			<action name="printRentalRefund" class="com.operations.commtransactions.rentalrefund.ClsRentalRefundAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">rentalRefundPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
			<action name="printRefundCheque" class="com.operations.commtransactions.rentalrefund.ClsRentalRefundAction" method="printChequeAction">
				<interceptor-ref name="params"/>
				<result name="print">../../../../com/operations/chequePrintingFormat.jsp</result>
			</action>
			
			<action name="rentalRefundDetails" class="com.operations.commtransactions.rentalrefund.ClsRentalRefundAction" method="saveAction">
			    <interceptor-ref name="params"/>
			    <result name="success" >../../../../com/operations/commtransactions/rentalrefund/rentalRefund.jsp</result>
			    <result name="fail">../../../../com/operations/commtransactions/rentalrefund/rentalRefund.jsp</result>
   			</action>
			
		</package>	
</struts>