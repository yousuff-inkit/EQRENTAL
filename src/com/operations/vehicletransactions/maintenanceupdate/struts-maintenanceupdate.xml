<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="maintenamce" extends="struts-default">
			<action name="saveMaint" class="com.operations.vehicletransactions.maintenanceupdate.ClsmaintenanceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >maintenanceUpdate.jsp</result>
				<result name="fail">maintenanceUpdate.jsp</result>
			</action>
			<action name="printMintupdate" class="com.operations.vehicletransactions.maintenanceupdate.ClsmaintenanceAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">maintupdateprint.jsp</result>
			</action>
		</package>	
</struts>