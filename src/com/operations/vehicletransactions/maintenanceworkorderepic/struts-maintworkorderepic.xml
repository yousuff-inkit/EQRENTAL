<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="mainworkorderepic" extends="struts-default">
			<action name="saveMaintworkorderepic" class="com.operations.vehicletransactions.maintenanceworkorderepic.ClsmaintWorkorderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >maintenanceworkorder.jsp</result>
				<result name="fail">maintenanceworkorder.jsp</result>
			</action>
			<action name="printmaintWorkepic" class="com.operations.vehicletransactions.maintenanceworkorderepic.ClsmaintWorkorderAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>      
			</action>
		</package>	
</struts>