<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="replacement" extends="struts-default">
			<action name="saveReplacement" class="com.operations.vehicletransactions.replacement.ClsReplacementAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >replacement.jsp</result>
				<result name="fail">replacement.jsp</result>
			</action>
	<action name="printReplacementss" class="com.operations.vehicletransactions.replacement.ClsReplacementAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>
				
			</action>
		</package>	
</struts>
