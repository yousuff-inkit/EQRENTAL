<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="movement" extends="struts-default">
			<action name="saveMovement" class="com.operations.vehicletransactions.movement.ClsMovementAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >movement.jsp</result>
				<result name="fail">movement.jsp</result>
			</action>
	<action name="printMovement" class="com.operations.vehicletransactions.movement.ClsMovementAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">movPrint.jsp</result> -->
				 <result name="print" >${url}</result> 
			</action>
		</package>	
</struts>
