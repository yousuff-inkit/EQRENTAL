<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="quickserviceupdate" extends="struts-default">
			<action name="saveQuickServiceUpdate" class="com.operations.vehicletransactions.quickserviceupdate.ClsQuickServiceUpdateAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >quickserviceupdate.jsp</result>
				<result name="fail">quickserviceupdate.jsp</result>
			</action>
	
		</package>	
</struts>
