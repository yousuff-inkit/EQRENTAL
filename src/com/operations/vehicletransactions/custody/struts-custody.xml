<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveCostmaster" />

	<package name="custody" extends="struts-default">
			<action name="saveCustody" class="com.operations.vehicletransactions.custody.ClsCustodyAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >custodyMaster.jsp</result>
				<result name="fail">custodyMaster.jsp</result>
			</action>
	<action name="printCustody" class="com.operations.vehicletransactions.custody.ClsCustodyAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >custodyPrint.jsp</result>
				
			</action>		
			
		</package>	
</struts>
