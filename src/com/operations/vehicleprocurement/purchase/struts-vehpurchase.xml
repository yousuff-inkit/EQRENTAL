<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="purchase" extends="struts-default">
			<action name="savePurchase" class="com.operations.vehicleprocurement.purchase.ClsvehpurchaseAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >purchase.jsp</result>
				<result name="fail">purchase.jsp</result>
			</action>
				<action name="printPurchase" class="com.operations.vehicleprocurement.purchase.ClsvehpurchaseAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>
			
		</package>	
</struts>