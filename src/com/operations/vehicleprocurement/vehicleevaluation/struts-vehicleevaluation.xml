<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="vehicleevaluation1" extends="struts-default">
			<action name="saveVehicleevaluation1" class="com.operations.vehicleprocurement.vehicleevaluation.ClsVehicleEvaluationAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >vehicleevaluation.jsp</result>
				<result name="fail">vehicleevaluation.jsp</result>
			</action>
	
	 <action name="printVehicleevaluation1" class="com.operations.vehicleprocurement.vehicleevaluation.ClsVehicleEvaluationAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">vehvaluationprint.jsp</result>
			</action> 
		</package>	
</struts>
