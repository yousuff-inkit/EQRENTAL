<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="vehpuchase" extends="struts-default">
			<action name="savepurreq" class="com.operations.vehicleprocurement.purchaserequest.ClsvehpurchasereqAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >PurchaseRequest.jsp</result>
				<result name="fail">PurchaseRequest.jsp</result>
			</action>
			
			<action name="printPurreq" class="com.operations.vehicleprocurement.purchaserequest.ClsvehpurchasereqAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">reqprint.jsp</result>
			</action>
			
		</package>	
</struts>