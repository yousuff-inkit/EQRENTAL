<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.devMode" value="false" />
	<package name="client" extends="struts-default">
			<action name="saveClientMaster" class="com.operations.clientrelations.client.ClsClientAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >clientMaster.jsp</result>
				<result name="fail">clientMaster.jsp</result>
			</action>
			
		   <action name="clientAudit" class="com.operations.clientrelations.client.ClsClientAction" method="saveAction">
			    <interceptor-ref name="params"/>
			    <result name="success" >../../../../com/operations/clientrelations/client/clientMaster.jsp</result>
			    <result name="fail">../../../../com/operations/clientrelations/client/clientMaster.jsp</result>
   			</action>
			
		</package>	
		
</struts>