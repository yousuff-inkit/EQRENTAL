<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Leasepeq" extends="struts-default">
			<action name="saveLeasepricereq" class="com.operations.marketing.leasepricerequest.ClsLeasepriceRequestAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >leasepricerequest.jsp</result>
				<result name="fail">leasepricerequest.jsp</result>
			</action>
			<action name="printLeasepricereq" class="com.operations.marketing.leasepricerequest.ClsLeasepriceRequestAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">leaseprint.jsp</result>
			</action>
		</package>	
</struts>