			</action>
			<action name="printLeaseCalculator1" class="com.operations.marketing.leasecalculator.ClsLeaseCalculatorAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result> 
			</action> 


			<action name="printcalQuot1" class="com.operations.marketing.leasecalculator.ClsLeaseCalculatorAction" method="printQotAction">
			    <interceptor-ref name="params"/>
			 <!--     <result name="print">savepdf</result> epicquotprint.jsp progressquotprint.jsp-->
	 		      <result name="print">epicquotprint.jsp</result>
			</action>

		</package>	
</struts>
