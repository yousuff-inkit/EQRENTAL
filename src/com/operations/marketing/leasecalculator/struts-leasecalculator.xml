<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="leasecalculator" extends="struts-default">
			<action name="saveLeaseCalculator1" class="com.operations.marketing.leasecalculator.ClsLeaseCalculatorAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >leaseCalculator.jsp</result>
