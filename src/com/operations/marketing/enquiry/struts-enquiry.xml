<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Enquiry" extends="struts-default">
			<action name="saveEnquiry" class="com.operations.marketing.enquiry.ClsEnquiryAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >enquiry.jsp</result>
				<result name="fail">enquiry.jsp</result>
			</action>
				<action name="printEnq" class="com.operations.marketing.enquiry.ClsEnquiryAction" method="printAction">
			    <interceptor-ref name="params"/>
		    <result name="print">${url}</result>
			</action>
		</package>	
</struts>