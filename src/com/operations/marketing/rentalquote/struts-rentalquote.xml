<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="rentalquote" extends="struts-default">
			<action name="saveRentalQuote" class="com.operations.marketing.rentalquote.ClsRentalQuoteAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >rentalQuote.jsp</result>
				<result name="fail">rentalQuote.jsp</result>
			</action>
			<action name="printRentalQuote" class="com.operations.marketing.rentalquote.ClsRentalQuoteAction" method="printAction">    
					<interceptor-ref name="params"/>
					<result name="print" >${url}</result> 
			   </action>
			<action name="printRentalQuote" class="com.operations.marketing.rentalquote.ClsRentalQuoteAction" method="printQtAction">    
					<interceptor-ref name="params"/>
					<result name="print" >${url}</result> 
			   </action>
		</package>	
</struts>