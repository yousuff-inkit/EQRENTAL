<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="equippurchase" extends="struts-default">
			<action name="saveequippurreq" class="com.equipment.equippurchaserequest.ClsEquipPurchaseRequestAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >PurchaseRequest.jsp</result>
				<result name="fail">PurchaseRequest.jsp</result>
			</action>
			
			<action name="printeqPurreq" class="com.equipment.equippurchaserequest.ClsEquipPurchaseRequestAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">reqprint.jsp</result>
			</action>
			
		</package>	
</struts>