<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" /> 
	

	<package name="eqpurchase" extends="struts-default">
			<action name="saveeqPurchase" class="com.equipment.equippurchase.ClsEquipPurchaseAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >purchase.jsp</result>
				<result name="fail">purchase.jsp</result>
			</action>
				<action name="printeqPurchase" class="com.equipment.equippurchase.ClsEquipPurchaseAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>
			
		</package>	
</struts>