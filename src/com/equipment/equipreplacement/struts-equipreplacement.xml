<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="equipreplacement" extends="struts-default">
			<action name="saveEquipReplacement" class="com.equipment.equipreplacement.ClsEquipReplacementAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >replacement.jsp</result>
				<result name="fail">replacement.jsp</result>
			</action>
	<action name="printEquipReplacement" class="com.equipment.equipreplacement.ClsEquipReplacementAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >replacementPrint.jsp</result>
				
			</action>
		</package>	
</struts>
