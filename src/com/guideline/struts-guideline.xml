<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="guideline" extends="struts-default">

		<action name="guideline" class="com.guideline.ClsGuidelineAction" method="saveGuideline">
		<interceptor-ref name="params"/>
			<result name="success">guideline.jsp</result>
			<result name="fail">guideline.jsp</result>
		</action>
		
		<action name="viewGuideline" class="com.guideline.ClsGuidelineAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">viewGuideline.jsp</result>
			</action>
			
			 <action name="viewDashBoardGuideline" class="com.guideline.ClsGuidelineAction" method="printDashBoardAction">
				<interceptor-ref name="params"/>
				<result name="print">viewDashBoardGuideline.jsp</result>
			</action>

	</package>
</struts>

