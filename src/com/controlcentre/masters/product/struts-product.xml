<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<constant name="struts.custom.i18n.resources" value="global" />
 
	
	<package name="product" extends="struts-default">
			<action name="saveProduct" class="com.controlcentre.masters.product.ClsProductAction" method="saveProductAction">
				<interceptor-ref name="params"/>
					
              
			  <result name="success" >product.jsp</result>
				<result name="fail">product.jsp</result>  
				
			 
			</action>
			
			
			<action name="saveProduct1" class="com.controlcentre.masters.product.ClsProductAction" method="saveProductAction">
				<interceptor-ref name="params"/>
					
              
			  <result name="success" >productarabic.jsp</result>
				<result name="fail">productarabic.jsp</result>  
				
			 
			</action>
					</package>	
</struts>