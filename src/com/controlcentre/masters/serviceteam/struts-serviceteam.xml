<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="serviceteam" />

	<package name="serviceteam" extends="struts-default">
			<action name="saveServiceteam" class="com.controlcentre.masters.serviceteam.ClsServiceTeamAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">serviceteam.jsp</result>
				<result name="fail">serviceteam.jsp</result>
			</action>
			 
		</package>	
</struts>
