<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveSalesman" />

	<package name="salesman" extends="struts-default">
			<action name="saveSalesman" class="com.controlcentre.masters.salesmanmaster.salesman.ClsSalesmanAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >salesManMaster.jsp</result>
				<result name="fail">salesManMaster.jsp</result>
			</action>
			
			
		</package>	
</struts>

