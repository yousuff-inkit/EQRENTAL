<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="authority" extends="struts-default">
			<action name="saveActionAuthority" class="com.controlcentre.masters.vehiclemaster.authority.ClsAuthorityAction" method="saveAction">
