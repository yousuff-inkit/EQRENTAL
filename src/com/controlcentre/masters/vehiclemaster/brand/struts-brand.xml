<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveBrand" />

	<package name="brand" extends="struts-default">
			<action name="saveBrand" class="com.controlcentre.masters.vehiclemaster.brand.ClsBrandAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >brand.jsp</result>
				<result name="fail">brand.jsp</result>
			</action>
			
			<action name="searchDetails" class="com.controlcentre.masters.vehiclemaster.brand.ClsBrandAction" method="searchDetails">
				<interceptor-ref name="params"/>
				<result name="success" >brand.jsp</result>
				<result name="fail">brand.jsp</result>
			</action>
		</package>	
</struts>

