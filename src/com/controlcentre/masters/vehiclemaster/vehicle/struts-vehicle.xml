<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="vehicle" extends="struts-default">
			<action name="saveVehicle" class="com.controlcentre.masters.vehiclemaster.vehicle.ClsVehicleAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >vehicle2.jsp</result>
				<result name="fail">vehicle2.jsp</result>
			</action>
			
				<action name="saveVehicle1" class="com.controlcentre.masters.vehiclemaster.vehicle.ClsVehicleAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >../../../../com/controlcentre/masters/vehicle/Copyvehicle2.jsp</result>
				<result name="fail">../../../../com/controlcentre/masters/vehicle/Copyvehicle2.jsp</result>
			</action>
		</package>	
</struts>

