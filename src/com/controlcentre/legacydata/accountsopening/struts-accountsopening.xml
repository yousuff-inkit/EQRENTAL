<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="accountsOpening" extends="struts-default">
			<action name="saveAccountsOpening" class="com.controlcentre.legacydata.accountsopening.ClsAccountsOpeningAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >accountsOpening.jsp</result>
				<result name="fail">accountsOpening.jsp</result>
			</action>
			
		</package>	
</struts>