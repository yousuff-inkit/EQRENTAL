<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="location" extends="struts-default">
			<action name="saveActionLocation" class="com.controlcentre.settings.companysettings.location.ClsLocationAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >location.jsp</result>
				<result name="fail">location.jsp</result>
			</action>
			
		</package>	
</struts>