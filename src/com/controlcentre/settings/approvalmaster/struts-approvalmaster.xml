<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="approvalmaster" extends="struts-default">
			<action name="saveApprovalMaster" class="com.controlcentre.settings.approvalmaster.ClsApprovalMasterAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >approvalMaster.jsp</result>
				<result name="fail">approvalMaster.jsp</result>
			</action>
			
		</package>	
</struts>