<struts>
	<constant name="struts.devMode" value="true" />

	<package name="userrolemaster" extends="struts-default">
			<action name="saveUserRoleMaster" class="com.controlcentre.settings.userrolemaster.ClsUserRoleAction" method="saveAction">
