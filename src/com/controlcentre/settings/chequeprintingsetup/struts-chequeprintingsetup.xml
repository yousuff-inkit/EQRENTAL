<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="chequeprintingsetup" extends="struts-default">
			<action name="saveChequePrintingSetUp" class="com.controlcentre.settings.chequeprintingsetup.ClsChequeSetUpAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >chequePrintingSetUp.jsp</result>
				<result name="fail">chequePrintingSetUp.jsp</result>
			</action>
			
			<action name="printChequeVoucher" class="com.controlcentre.settings.chequeprintingsetup.ClsChequeSetUpAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">chequePrintingFormat.jsp</result>
			</action>
			
		</package>	
</struts>