<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="currencybook" extends="struts-default">
			<action name="saveCurrencyBook" class="com.controlcentre.settings.currencybook.ClsCurrencyBookAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">currencyBook.jsp</result>
				<result name="fail">currencyBook.jsp</result>
			</action>
			
		</package>	
</struts>