<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Purchaseinv" extends="struts-default"> 
			<action name="saveActionpurInv" class="com.procurement.purchase.purchaseinvoice.ClspurchaseinvoiceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >purchseinvoicemaster.jsp</result>
				<result name="fail">purchseinvoicemaster.jsp</result>
			</action>
			
			
			<action name="printpurchase" class="com.procurement.purchase.purchaseinvoice.ClspurchaseinvoiceAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action> 
			
			
			
		</package>	
		
		
		
</struts>

