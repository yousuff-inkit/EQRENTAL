<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="purchaseOrder" extends="struts-default">
			<action name="saveActionpurOrders" class="com.procurement.purchase.purchaseorder.ClspurchaseorderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">PurchaseOrder.jsp</result>
				<result name="fail">PurchaseOrder.jsp</result>
			</action>
			
				
	
			<action name="jasperprintpurchaseorder" class="com.procurement.purchase.purchaseorder.ClspurchaseorderAction" method="jasperprintAction">
			    <interceptor-ref name="params"/>
			   
			</action>

		<action name="printpurchaseorder1" class="com.procurement.purchase.purchaseorder.ClspurchaseorderAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print" >${url}</result> 
			</action>
			
		</package>	
		
		
		
</struts>

