<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Purchaseinvret" extends="struts-default"> 
			<action name="saveActionpurInvret" class="com.procurement.purchase.purchaseinvoicereturn.ClspurchaseinvoicereturnAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >purchseinvoicereturnmaster.jsp</result>
