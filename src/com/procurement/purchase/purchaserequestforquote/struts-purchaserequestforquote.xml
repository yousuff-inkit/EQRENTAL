<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="purchaseReqforqout" extends="struts-default">
			<action name="savepurchaseReqforqout" class="com.procurement.purchase.purchaserequestforquote.ClspurchaserequestforquoteAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >purchaserequestforquote.jsp</result>
				<result name="fail">purchaserequestforquote.jsp</result>
			</action>
			
				
  <action name="printpurchaseReqforqout" class="com.procurement.purchase.purchaserequestforquote.ClspurchaserequestforquoteAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">fancyrfqprint.jsp</result>
			</action>
	 
		</package>	
		
		
		
</struts>

