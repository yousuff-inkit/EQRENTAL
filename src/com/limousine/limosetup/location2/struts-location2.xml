<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="location2" extends="struts-default">
			<action name="saveLocation2" class="com.limousine.limosetup.location2.ClsLocation2Action" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >location2.jsp</result>
				<result name="fail">location2.jsp</result>
			</action>
		</package>	
</struts>
