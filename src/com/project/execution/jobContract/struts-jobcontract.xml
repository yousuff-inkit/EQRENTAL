<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="jobcontract" extends="struts-default">
			<action name="saveJobcontract" class="com.project.execution.jobContract.ClsJobContractAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >jobcontract.jsp</result>
				<result name="fail">jobcontract.jsp</result>
			</action>
			
			<action name="printJobContract" class="com.project.execution.jobContract.ClsJobContractAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result> 
			</action>	
		
		</package>	
</struts>