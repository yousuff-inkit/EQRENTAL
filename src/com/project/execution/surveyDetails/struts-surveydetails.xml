<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="surveydetil" extends="struts-default">
			<action name="saveSurveydetails" class="com.project.execution.surveyDetails.ClsSurveyDetailsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >surveydet.jsp</result>
				<result name="fail">surveydet.jsp</result>
			</action>
				<action name="printSurvey" class="com.project.execution.surveyDetails.ClsSurveyDetailsAction" method="printAction">
			    <interceptor-ref name="params"/>
		    <result name="print">${url}</result>
			</action>
		</package>	
</struts>