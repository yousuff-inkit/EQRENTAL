<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="estimation" extends="struts-default">
			<action name="saveEstimation" class="com.project.execution.estimation.ClsEstimationAction" method="saveEstimateAction">
				<interceptor-ref name="params"/>
				<result name="success" >estimation.jsp</result>
				<result name="fail">estimation.jsp</result>
			</action>
		
		</package>	
</struts>