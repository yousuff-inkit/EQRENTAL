<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="projectinvoicereturn" extends="struts-default">
	
			<action name="saveProjectinvoicereturn" class="com.project.execution.projectInvoicereturn.ClsProjectInvoiceReturnAction" method="saveInvoiceAction">
				<interceptor-ref name="params"/>
				<result name="success">invoiceReturn.jsp</result>
				<result name="fail">invoiceReturn.jsp</result>
			</action>
			
			<action name="printInvoiceReturn" class="com.project.execution.projectInvoicereturn.ClsProjectInvoiceReturnAction" method="printAction">
				<!-- <interceptor-ref name="params"/> -->
				<result name="print" >${url}</result> 
			</action>
		</package>	
</struts>