<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="completioncertificate" extends="struts-default">
	
			<action name="savecompletioncertificate" class="com.project.execution.completioncertificate.ClsCompletionCertificateAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >completioncertificate.jsp</result>
				<result name="fail">completioncertificate.jsp</result>
			</action>
			
		
		</package>	
</struts>