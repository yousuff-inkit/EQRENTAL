<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="servicecontract" extends="struts-default">
			<action name="saveServicecontractpd" class="com.project.execution.serviceContract.ClsServiceContractAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >servicecontract.jsp</result>
				<result name="fail">servicecontract.jsp</result>
			</action>
			
			<action name="printAmcContract" class="com.project.execution.serviceContract.ClsServiceContractAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result> 
			</action>
			<action name="PRINTamcContract" class="com.project.execution.serviceContract.ClsServiceContractAction" method="jasperprintAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print" >${url}</result> --> 
			</action>	
		
		</package>	
</struts>