<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="fixedassetposting" extends="struts-default">
			<action name="fixedassetdepreciationposting" class="com.fixedassets.assetposting.fixedassetposting.ClsFixedAssetDepreciationPostingAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">fixedAssetPosting.jsp</result>
				<result name="fail">fixedAssetPosting.jsp</result>
			</action>
			
			<action name="printFixedAssetPosting" class="com.fixedassets.assetposting.fixedassetposting.ClsFixedAssetDepreciationPostingAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>