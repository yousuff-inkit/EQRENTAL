<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="assetdisposal" extends="struts-default">
			<action name="saveAssetDisposal" class="com.fixedassets.assets.assetdisposal.ClsAssetDisposalAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >assetdisposal.jsp</result>
				<result name="fail">assetdisposal.jsp</result>
			</action>
			<action name="printAssetDisposal" class="com.fixedassets.assets.assetdisposal.ClsAssetDisposalAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">invoiceRecieptNoori.jsp</result> -->
				<result name="print" >${url}</result> 
			</action>
	
			
					</package>	
</struts>