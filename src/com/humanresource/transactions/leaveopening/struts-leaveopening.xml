<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="leaveopening" extends="struts-default">
			<action name="saveLeaveOpening" class="com.humanresource.transactions.leaveopening.ClsLeaveOpeningAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">leaveOpening.jsp</result>
				<result name="fail">leaveOpening.jsp</result>
			</action>
			
		</package>	
</struts>

