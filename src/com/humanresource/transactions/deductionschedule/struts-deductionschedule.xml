<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="DeductionSchedule" extends="struts-default">
			<action name="saveDeductionSchedule" class="com.humanresource.transactions.deductionschedule.ClsDeductionScheduleAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">deductionSchedule.jsp</result>
				<result name="fail">deductionSchedule.jsp</result>
			</action>
	<action name="printDeduction" class="com.humanresource.transactions.deductionschedule.ClsDeductionScheduleAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
		</package>	
</struts>

