<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="terminationbenefitsposting" extends="struts-default">
			<action name="terminalbenefitsposting" class="com.humanresource.transactions.terminationbenefits.ClsTerminationBenefitsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">terminationBenefits.jsp</result>
				<result name="fail">terminationBenefits.jsp</result>
			</action>
			
			<action name="printTerminalBenefitsPosting" class="com.humanresource.transactions.terminationbenefits.ClsTerminationBenefitsAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>