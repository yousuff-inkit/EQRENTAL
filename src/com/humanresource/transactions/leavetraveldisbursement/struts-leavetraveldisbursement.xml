<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="leavetraveldisbursement" extends="struts-default">
			<action name="saveLeaveTravelDisbursement" class="com.humanresource.transactions.leavetraveldisbursement.ClsLeaveTravelDisbursementAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">leaveTravelDisbursement.jsp</result>
				<result name="fail">leaveTravelDisbursement.jsp</result>
			</action>
			
			<action name="printLeaveTravelDisbursement" class="com.humanresource.transactions.leavetraveldisbursement.ClsLeaveTravelDisbursementAction" method="printAction">
				<interceptor-ref name="params"/>
			</action>
			
		</package>	
</struts>

