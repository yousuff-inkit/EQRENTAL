<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="leaverequest" extends="struts-default">
			<action name="saveLeaveRequests" class="com.humanresource.transactions.leaverequest.ClsLeaveRequestAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">leaveRequest.jsp</result>
				<result name="fail">leaveRequest.jsp</result>
			</action>
			<action name="printLeaveRequests" class="com.humanresource.transactions.leaverequest.ClsLeaveRequestAction" method="printAction">
				<interceptor-ref name="params"/>
			    <result name="print" >${url}</result>
			</action>
			

		</package>	
</struts>

