<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.devMode" value="false" />
	<package name="designationsetup" extends="struts-default">
			<action name="saveGrd" class="com.humanresource.setup.designationsetup.ClsdesignationsetupAction" method="saveAction1">
				<interceptor-ref name="params"/>
				<result name="success" >grade.jsp</result>
				<result name="fail">grade.jsp</result>
			</action>
			<action name="saveqlf" class="com.humanresource.setup.designationsetup.ClsdesignationsetupAction" method="saveAction2">
				<interceptor-ref name="params"/>
			   <result name="success" >qualification.jsp</result>
				<result name="fail">qualification.jsp</result>
			</action>
		</package>	
		
</struts>