<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Leave" extends="struts-default">
			<action name="saveLeave" class="com.humanresource.setup.hrsetup.leave.ClsLeaveAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >leave.jsp</result>
				<result name="fail">leave.jsp</result>
			</action>
		
			
		</package>	
</struts>