<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="icBankReceipt" extends="struts-default">
			<action name="saveIcBankReceipt" class="com.finance.intercompanytransactions.icbankreceipt.ClsIcBankReceiptAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">icBankReceipt.jsp</result>
				<result name="fail">icBankReceipt.jsp</result>
			</action>
			<action name="printIcBankReceipt" class="com.finance.intercompanytransactions.icbankreceipt.ClsIcBankReceiptAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/cashVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>