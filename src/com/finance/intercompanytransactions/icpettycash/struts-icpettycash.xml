<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="IcPettyCash" extends="struts-default">
			<action name="saveIcPettyCash" class="com.finance.intercompanytransactions.icpettycash.ClsIcPettyCashAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">icPettyCash.jsp</result>
				<result name="fail">icPettyCash.jsp</result>
			</action>
			
			<action name="printIcPettyCashPayment" class="com.finance.intercompanytransactions.icpettycash.ClsIcPettyCashAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/pettyCashVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>