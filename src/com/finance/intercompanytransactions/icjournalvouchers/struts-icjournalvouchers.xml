<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="icjournalvouchers" extends="struts-default">
			<action name="saveIcJournalVoucher" class="com.finance.intercompanytransactions.icjournalvouchers.ClsIcJournalVouchersAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">icJournalVoucher.jsp</result>
				<result name="fail">icJournalVoucher.jsp</result>
			</action>
			
			<action name="printIcJournalVoucher" class="com.finance.intercompanytransactions.icjournalvouchers.ClsIcJournalVouchersAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>