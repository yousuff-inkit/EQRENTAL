<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="icCashReceipt" extends="struts-default">
			<action name="saveIcCashReceipt" class="com.finance.intercompanytransactions.iccashreceipt.ClsIcCashReceiptAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">icCashReceipt.jsp</result>
				<result name="fail">icCashReceipt.jsp</result>
			</action>
			<action name="printIcCashReceipt" class="com.finance.intercompanytransactions.iccashreceipt.ClsIcCashReceiptAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/cashVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>