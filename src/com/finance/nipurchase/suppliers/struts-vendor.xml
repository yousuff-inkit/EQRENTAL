<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="suppliers" extends="struts-default">
			<action name="saveVendorDetails" class="com.finance.nipurchase.suppliers.ClsVendorDetailsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >vendorDetails.jsp</result>
				<result name="fail">vendorDetails.jsp</result>
			</action>
			
		</package>	
</struts>