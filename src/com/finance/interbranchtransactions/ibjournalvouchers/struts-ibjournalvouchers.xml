<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="ibjournalvouchers" extends="struts-default">
			<action name="saveIbJournalVoucher" class="com.finance.interbranchtransactions.ibjournalvouchers.ClsIbJournalVouchersAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >ibJournalVoucher.jsp</result>
				<result name="fail">ibJournalVoucher.jsp</result>
			</action>
			
			<action name="printIbJournalVoucher" class="com.finance.interbranchtransactions.ibjournalvouchers.ClsIbJournalVouchersAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/ibJournalVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>