<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="ibCashPayment" extends="struts-default">
			<action name="saveIbCashPayment" class="com.finance.interbranchtransactions.ibcashpayment.ClsIbCashPaymentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >ibCashPayment.jsp</result>
				<result name="fail">ibCashPayment.jsp</result>
			</action>
			
			<action name="printIBCashPayment" class="com.finance.interbranchtransactions.ibcashpayment.ClsIbCashPaymentAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/ibCashVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>