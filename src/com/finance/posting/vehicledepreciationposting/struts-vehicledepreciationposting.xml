<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="vehicledepreciationposting" extends="struts-default">
			<action name="vehicledepreciationposting" class="com.finance.posting.vehicledepreciationposting.ClsVehicleDepreciationPostingAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >vehicleDepreciationPosting.jsp</result>
				<result name="fail">vehicleDepreciationPosting.jsp</result>
			</action>
			
			<action name="printVehicleDepreciationPosting" class="com.finance.posting.vehicledepreciationposting.ClsVehicleDepreciationPostingAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>