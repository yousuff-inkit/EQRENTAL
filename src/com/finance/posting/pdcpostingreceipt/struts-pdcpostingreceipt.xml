<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="PDCPostingReceipt" extends="struts-default">
			<action name="savePDCPostingReceipt" class="com.finance.posting.pdcpostingreceipt.ClsPDCPostingReceiptAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >pdcPostingReceipt.jsp</result>
				<result name="fail">pdcPostingReceipt.jsp</result>
			</action>
			
		</package>	
</struts>