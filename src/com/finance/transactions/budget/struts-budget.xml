<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	
	<package name="budget" extends="struts-default">
			<action name="saveBudget" class="com.finance.transactions.budget.ClsBudgetAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">budget.jsp</result>
				<result name="fail">budget.jsp</result>
			</action>
			
			<action name="printBudget" class="com.finance.transactions.budget.ClsBudgetAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>