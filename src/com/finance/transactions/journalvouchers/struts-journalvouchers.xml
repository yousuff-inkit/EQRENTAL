<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="journalvouchers" extends="struts-default">
			<action name="saveJournalVoucher" class="com.finance.transactions.journalvouchers.ClsJournalVouchersAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >journalVoucher.jsp</result>
				<result name="fail">journalVoucher.jsp</result>
			</action>
			
			<action name="printJournalVoucher" class="com.finance.transactions.journalvouchers.ClsJournalVouchersAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/journalVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			

<action name="JournalVoucherPrint" class="com.finance.transactions.journalvouchers.ClsJournalVouchersAction" method="jasperprintAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/journalVoucherPrint.jsp</result> -->
				<!-- <result name="print">${url}</result> -->
			</action>
		</package>	
</struts>