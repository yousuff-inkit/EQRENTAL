<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="cashReceipt" extends="struts-default">
			<action name="saveCashReceipt" class="com.finance.transactions.cashreceipt.ClsCashReceiptAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >cashReceipt.jsp</result>
				<result name="fail">cashReceipt.jsp</result>
			</action>
			<action name="printCashReceipt" class="com.finance.transactions.cashreceipt.ClsCashReceiptAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/cashVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>