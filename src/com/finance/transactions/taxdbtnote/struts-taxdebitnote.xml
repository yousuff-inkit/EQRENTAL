<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="taxdbtnote" extends="struts-default">
			<action name="saveTaxDebitNote" class="com.finance.transactions.taxdbtnote.ClsTaxDebitNoteAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >taxDebitNote.jsp</result>
				<result name="fail">taxDebitNote.jsp</result>
			</action>
			
			<action name="printTaxDebitNote" class="com.finance.transactions.taxdbtnote.ClsTaxDebitNoteAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/creditAndDebitPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>