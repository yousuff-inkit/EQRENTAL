<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="contratrans" extends="struts-default">
			<action name="saveContraTrans" class="com.finance.transactions.contratrans.ClsContraTransAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >contraTrans.jsp</result>
				<result name="fail">contraTrans.jsp</result>
			</action>
			
			<action name="printContraTrans" class="com.finance.transactions.contratrans.ClsContraTransAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/contraTransPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
		</package>	
</struts>