<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="multiplechequepreparation" extends="struts-default">
			<action name="saveMultipleChequePreparation" class="com.finance.transactions.multiplechequepreparation.ClsMultipleChequePreparationAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">multipleChequePreparation.jsp</result>
				<result name="fail">multipleChequePreparation.jsp</result>
			</action>

		</package>	
</struts>