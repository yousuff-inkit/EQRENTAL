<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveAccountmaster" />

	<package name="accountmaster" extends="struts-default">
			<action name="saveAccountmaster" class="com.finance.accountssetup.accountsMaster.ClsAccMasterAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >accountsMaster.jsp</result>
				<result name="fail">accountsMaster.jsp</result>
			</action>
			
			
		</package>	
</struts>
